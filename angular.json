{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1, 
  "newProjectRoot": "projects",
  "projects": {
    "done-page-selector": {
      "projectType": "library",
      "root": "projects/done-page-selector",
      "sourceRoot": "projects/done-page-selector/src",
      "prefix": "done",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/done-page-selector/tsconfig.lib.json",
            "project": "projects/done-page-selector/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/done-page-selector/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/done-page-selector/src/test.ts",
            "tsConfig": "projects/done-page-selector/tsconfig.spec.json",
            "karmaConfig": "projects/done-page-selector/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/done-page-selector/tsconfig.lib.json",
              "projects/done-page-selector/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo1": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/demo1",
      "sourceRoot": "projects/demo1/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo1",
            "index": "projects/demo1/src/index.html",
            "main": "projects/demo1/src/main.ts",
            "polyfills": "projects/demo1/src/polyfills.ts",
            "tsConfig": "projects/demo1/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/demo1/src/favicon.ico",
              "projects/demo1/src/assets"
            ],
            "styles": [
              "projects/demo1/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo1/src/environments/environment.ts",
                  "with": "projects/demo1/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo1:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo1:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo1:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo1/src/test.ts",
            "polyfills": "projects/demo1/src/polyfills.ts",
            "tsConfig": "projects/demo1/tsconfig.spec.json",
            "karmaConfig": "projects/demo1/karma.conf.js",
            "assets": [
              "projects/demo1/src/favicon.ico",
              "projects/demo1/src/assets"
            ],
            "styles": [
              "projects/demo1/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demo1/tsconfig.app.json",
              "projects/demo1/tsconfig.spec.json",
              "projects/demo1/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/demo1/e2e/protractor.conf.js",
            "devServerTarget": "demo1:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo1:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "done-page-selector"
}
